<h1>Bikes#new</h1>
<p>Find me in app/views/bikes/new.html.erb</p>

<%= simple_form_for [@location, @bike] do |f| %>
  <%= f.input :make %>
  <%= f.input :model %>
  <%= f.input :engine %>
  <%= f.input :year %>
  <%= f.input :bike_type %>
  <%= f.input :rate_daily %>
  <%= f.input :address %>
  <%= f.input :bike_image %>
  <%= f.button :submit %>
<% end %>
